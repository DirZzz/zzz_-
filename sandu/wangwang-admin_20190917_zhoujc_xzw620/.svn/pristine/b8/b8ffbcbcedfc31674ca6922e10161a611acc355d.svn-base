        case fhdp.source_type
        when 1 then '内部制作'
        when 2 then '装进我家'
        end as sourceName,
        fhdp.source_plan_id as sourcePlanId,
        fhdp.open_state as openState,
        fhdp.vr_resource_uuid as vrResourceUuid,
        fhdp.version as version,
        fhdp.creator as creator,
