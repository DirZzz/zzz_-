      open_state = #{openState,jdbcType=INTEGER},
      vr_resource_uuid = #{vrResourceUuid,jdbcType=VARCHAR},
      version = #{version,jdbcType=INTEGER},
      creator = #{creator,jdbcType=VARCHAR},
      gmt_create = #{gmtCreate,jdbcType=TIMESTAMP},
      modifier = #{modifier,jdbcType=VARCHAR},
      gmt_modified = #{gmtModified,jdbcType=TIMESTAMP},
      is_deleted = #{isDeleted,jdbcType=INTEGER},
      remark = #{remark,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>
    <update id="batchUpdatePlanSecrecy">
        update full_house_design_plan set open_state = #{secrecyFlag},open_time = now()
        where 1= 1
        <choose>
            <when test="planIds!= null and planIds.size > 0">
                and id in
                <foreach collection="planIds" item="planId" separator="," open="(" close=")">
                    #{planId}
                </foreach>
            </when>
            <otherwise>
                and 1=2
            </otherwise>
        </choose>
    </update>

    <update id="updateSalePrice">
     UPDATE
       full_house_design_plan
       SET
        sale_price = #{salePrice},
        sale_price_charge_type = #{salePriceChargeType},
        gmt_modified = now()
        WHERE
        id =#{id}
  </update>

    <update id="updatePlanPrice">
        UPDATE
          full_house_design_plan
        SET
          plan_price = #{planPrice},
          charge_type = #{chargeType},
          gmt_modified = now()
        WHERE
          id =#{id}
  </update>

    <!--
        and (
        exists (select 1 from  design_plan_2c_platform dp2cp where cp.plan_id = dp2cp.plan_id and dp2cp.is_deleted = 0 and dp2cp.putaway_state  = 1)
        or exists(select 1 from  design_plan_2b_platform dp2bp where cp.plan_id = dp2bp.plan_id and dp2bp.is_deleted = 0 and dp2bp.putaway_state  = 1)
        )
    -->
    <select id="selectManagerSXWFullHouseDesignPlan" resultType="com.sandu.api.solution.model.bo.DesignPlanBO" parameterType="com.sandu.api.solution.input.FullHouseDesignPlanQuery">
        select
          fp.plan_pic_id as picId,
          fp.plan_name,
          fp.plan_style_id,
          fp.gmt_modified,
          fp.source_type,
          bc.company_name,
          cs.shop_name,
          fp.sort,
          fp.shop_in720_page,
          fp.show_In_SXW_Flag,
          fp.plan_style_name as designStyleName,
          bc.id as companyId,
          fp.source_type as originId,
          fp.gmt_modified as completeDate,
          IF( psuper.id is null, 0,1) AS showInSXWIndexFlag,
          psuper.ordering as sortInSXWIndex,
          cp.plan_id as planId,
          cp.shop_id as shopId,
          fp.is_show_cover_pic_ids as isShowCoverPicIds
      from  company_shop_design_plan cp
      inner join (SELECT SUBSTRING_INDEX(GROUP_CONCAT(a.shop_id ORDER BY c.shop_type asc, a.gmt_modified asc),
        ",",
        1) as shop_id,
        SUBSTRING_INDEX(GROUP_CONCAT(a.gmt_modified ORDER BY a.gmt_modified desc),
        ",", 1) as order_time,
        GROUP_CONCAT(c.shop_name) as shopName,
        a.plan_id
        FROM company_shop_design_plan a
        INNER JOIN company_shop c ON a.shop_id = c.id
        inner join full_house_design_plan fhdp on fhdp.id = a.plan_id
        where a.is_deleted = 0
        and c.is_deleted = 0
        and a.plan_recommended_type = 3
      GROUP BY a.plan_id) t on t.plan_id = cp.plan_id and cp.shop_id = t.shop_id
      LEFT JOIN full_house_design_plan fp on cp.plan_id = fp.id
      left join company_shop cs on cp.shop_id = cs.id and find_in_set('2',cs.release_platform_values)
      left join base_company bc on fp.company_id = bc.id
      left join design_plan_recommended_superior psuper on psuper.design_plan_recommended_id = fp.id and psuper.is_deleted = 0 and psuper.plan_type = 1
      where cp.is_deleted = 0 and cp.plan_recommended_type = 3 and  fp.is_deleted =  0 and
      not exists (SELECT 1 from  full_house_design_plan_detail where fp.id = full_house_plan_id and  is_deleted = 1)
      <if test="queryName != null">
         AND fp.plan_name  like concat ('%', #{queryName},'%')
      </if>
      <if test="listGroupStyleId != null and listGroupStyleId.size > 0">
        and fp.plan_style_id in
        <foreach collection="listGroupStyleId" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
      </if>
      <if  test="companyName != null">
         and bc.company_name like concat ('%', #{companyName},'%')
      </if>
      <if test="shopName != null">
          and cs.shop_name  like concat ('%', #{shopName},'%')
      </if>
      <if test="planId != null">
          and cp.plan_id like concat('%',#{planId},'%')
      </if>
        <if test="showInSXWIndexFlag != null">
            <if test="showInSXWIndexFlag eq 1">
                and exists(select 1 from design_plan_recommended_superior s where s.design_plan_recommended_id = fp.id
                and s.is_deleted = 0 and s.plan_type = 1)
            </if>
            <if test="showInSXWIndexFlag eq 0">
                and not exists(select 1 from design_plan_recommended_superior s where s.design_plan_recommended_id =
                fp.id and s.is_deleted = 0 and s.plan_type = 1)
            </if>
        </if>
        <if test="showInSXWFlag != null">and fp.show_in_sxw_flag = #{showInSXWFlag}</if>
      group by cp.plan_id
      order by fp.sort desc,
      fp.gmt_modified desc
    </select>
    <select id="getPushStateByPlanIds" resultType="com.sandu.api.solution.model.bo.FullHouseDesignPlanBO">
        select plan_id as id,group_concat(shop_id) as shopIds from company_shop_design_plan where
        plan_id in
        <foreach collection="planIds" separator="," item="it" open="(" close=")">
            #{it}
        </foreach>
        and is_deleted = 0
        and plan_type = 2
        group by plan_id
    </select>    <update id="updatePutawayTime">
        UPDATE
          full_house_design_plan
        SET
          putaway_time = now()
        WHERE
          id =#{id}
  </update>

    <select id="queryDesignerListByCompanyId" resultType="com.sandu.api.solution.output.DesignerUserKVDto">
        SELECT DISTINCT fhdp.user_id as id,sys.user_name as userName FROM full_house_design_plan fhdp
        left join sys_user sys on fhdp.user_id = sys.id WHERE fhdp.is_deleted = 0 and fhdp.source_type != 2 AND fhdp.company_id = #{companyId}
    </select>


    <select id="selectListByPlands" resultType="com.sandu.api.solution.model.bo.FullHouseDesignPlanBO">
         select
        fhdp.id as id,
		fhdp.company_id AS companyId,
        fhdp.plan_code as planCode,
        fhdp.plan_name as planName,
        fhdp.plan_style_name as planStyleName,
        fhdp.brand_ids as brandId,
        fhdp.gmt_create as gmtCreate,
        sys.user_name as userName,
        fhdp.plan_pic_id as planPicId,
        case fhdp.source_type
        when 1 then '内部制作'
        when 2 then '装进我家'
        when 3 then '交付方案'
        when 4 then '分享方案'
        end as sourceName,
        fhdp.putaway_time as putawayTime
        from full_house_design_plan fhdp
        left join sys_user sys on fhdp.user_id = sys.id
        WHERE 1=1
        <if test="planIds!=null and planIds.size > 0">
            and fhdp.id in
            <foreach collection="planIds" item="item" open="(" close=")" separator=",">
                #{item}
            </foreach>
        </if>
         group by fhdp.id
    </select>

	<select id="selectFullHouseDesignPlanCoverPicInfoDOById" resultType="com.sandu.api.solution.output.FullHouseDesignPlanCoverPicInfoDO">
		select
		rrp.id as pic_id,
		rrp.pic_path as pic_path,
		fhdpd.space_type as space_type_value
		from full_house_design_plan fhdp
		left join full_house_design_plan_detail fhdpd on fhdpd.full_house_plan_id = fhdp.id
		left join design_plan_recommended dpr on dpr.id = fhdpd.recommended_plan_group_primary_id
		left join res_render_pic rrp on rrp.id = dpr.cover_pic_Id
		where fhdp.id = #{id}
		and fhdpd.is_deleted = 0;
	</select>

</mapper>