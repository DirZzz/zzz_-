<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.resmodel.dao.ModelUpdateTask2019Dao" >
  <resultMap id="BaseResultMap" type="com.sandu.api.resmodel.model.ModelUpdateTask2019" >
    <result column="model_id" property="modelId" jdbcType="BIGINT" />
    <result column="model_path" property="modelPath" jdbcType="VARCHAR" />
    <result column="file_key" property="fileKey" jdbcType="VARCHAR" />
    <result column="model_size" property="modelSize" jdbcType="VARCHAR" />
    <result column="state" property="state" jdbcType="INTEGER" />
    <result column="count" property="count" jdbcType="INTEGER" />
    <result column="type" property="type" jdbcType="INTEGER" />
  </resultMap>

  <select id="selectModelUpgradeId" resultType="java.lang.Integer">
      SELECT DISTINCT
          mu.model_id
      FROM
          model_update_task_wait mu,
          res_model rm,
          base_product bp
      WHERE
          mu.model_id = rm.id
          AND mu.state = 0
          AND rm.id = bp.windows_u3dmodel_id
  </select>

  <update id="updateModelState">
      UPDATE model_update_task_wait
      SET state = 1
      WHERE
          model_id IN
          <foreach collection="idList" item="item" open="(" close=")" separator=",">
              #{item}
          </foreach>
  </update>

  <insert id="insertModelUpgradeTask">
      INSERT into model_update_task_2019
      SELECT
          rm.id AS model_id,
          IF(rm.file_3du_path is null,IFNULL(rm.old_version_path,rm.model_path),rm.file_3du_path) AS model_path,
          rm.file_key,
          rm.model_size,
          0 AS state,
          0 AS count,
          locate('3DU',IF(rm.file_3du_path is null,IFNULL(rm.old_version_path,rm.model_path),rm.file_3du_path)) AS type
      FROM
          res_model rm
          INNER JOIN base_product bp ON rm.id = bp.windows_u3dmodel_id
      WHERE
          bp.fbx_state = 0
          AND product_type_value NOT IN (1,2)
          AND rm.id IN
          <foreach collection="idList" item="item" open="(" close=")" separator=",">
              #{item}
          </foreach>
  </insert>

  <insert id="insertSelectiveTaskWait" parameterType="com.sandu.api.resmodel.model.ModelUpdateTask2019" >
    insert into model_update_task_wait
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="modelId != null" >
        model_id,
      </if>
      <if test="state != null" >
        state,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="modelId != null" >
        #{modelId,jdbcType=BIGINT},
      </if>
      <if test="state != null" >
        #{state,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>

  <insert id="insertSelective" parameterType="com.sandu.api.resmodel.model.ModelUpdateTask2019" >
    insert into model_update_task_2019
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="modelId != null" >
        model_id,
      </if>
      <if test="modelPath != null" >
        model_path,
      </if>
      <if test="fileKey != null" >
        file_key,
      </if>
      <if test="modelSize != null" >
        model_size,
      </if>
      <if test="state != null" >
        state,
      </if>
      <if test="count != null" >
        count,
      </if>
      <if test="type != null" >
        type,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="modelId != null" >
        #{modelId,jdbcType=BIGINT},
      </if>
      <if test="modelPath != null" >
        #{modelPath,jdbcType=VARCHAR},
      </if>
      <if test="fileKey != null" >
        #{fileKey,jdbcType=VARCHAR},
      </if>
      <if test="modelSize != null" >
        #{modelSize,jdbcType=VARCHAR},
      </if>
      <if test="state != null" >
        #{state,jdbcType=INTEGER},
      </if>
      <if test="count != null" >
        #{count,jdbcType=INTEGER},
      </if>
      <if test="type != null" >
        #{type,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>

</mapper>