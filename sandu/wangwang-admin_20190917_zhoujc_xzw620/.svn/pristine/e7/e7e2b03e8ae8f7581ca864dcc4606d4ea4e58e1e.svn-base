    <select id="listProCtopsByProductId" resultType="com.sandu.api.product.model.BaseProductCountertops">
  	  	SELECT
	  pc.*,res_pic.pic_path as texturePicPath
	FROM
	  base_product_countertops pc
    left join res_texture texture on pc.texture_id = texture.id
    left join res_pic on texture.pic_id = res_pic.id
	WHERE pc.is_deleted = 0
	  AND pc.product_id = #{productId}
	  AND pc.data_type > 0
	ORDER BY pc.data_type ASC
  </select>

    <update id="deleteProCtops">
        UPDATE
        base_product_countertops pc
        SET
        pc.is_deleted = 1
        WHERE pc.id IN
        <foreach collection="proCtops" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </update>
