    <select id="findAll" resultMap="BaseResultMap" parameterType="com.sandu.api.basesupplydemand.input.BasesupplydemandQuery">
        select
        <include refid="Base_Column_List"/>
        from base_supply_demand
        where is_deleted=0
        <if test="supplyDemandCategoryId != null">
            AND CONCAT(',', supply_demand_category_id, ',') LIKE CONCAT('%,', #{supplyDemandCategoryId}, ',%')
        </if>
        <if test="queryParam != null">
            and (
            title like concat ('%',#{queryParam},'%') or
            creator like concat ('%',#{queryParam},'%')
            )
        </if>
        <if test="startTime != null">
            and gmt_publish &gt;= #{startTime}
        </if>
        <if test="endTime != null">
            and gmt_publish &lt;= #{endTime}
        </if>
        <if test="type != null">
            and type = #{type}
        </if>
        order by recommended_time desc,gmt_create desc
    </select>
    <select id="listNodeDetails" resultType="com.sandu.api.basesupplydemand.model.NodeDetails">
        select node.id as nodeId,node.content_id ,node.node_type,detail.detail_type as detailsType,detail.value from
        node_info
        node
        left join node_info_detail detail on node.id = detail.node_id
        where node.node_type = #{nodeType}
        and content_id in
        <foreach collection="contentIds" separator="," open="(" close=")" item="it">
            #{it}
        </foreach>
        <if test="detailTypes != null and detailTypes.size > 0">
            and detail_type in
            <foreach collection="detailTypes" separator="," close=")" open="(" item="it">
                #{it}
            </foreach>
        </if>
    </select>
    <select id="fetchPlanNameByDemandId" resultType="java.lang.String">
        select
        case
            when plan_type  in  (2,4) then (select plan_name from full_house_design_plan where id = t.plan_id)
            when plan_type =  1 then (select plan_name from design_plan_recommended where id = t.plan_id)
            when plan_type =  3 then (select plan_name from design_plan_render_scene where id = t.plan_id)
        end planName
        from
        (select info.plan_id,info.plan_type from demand_info_rel info
                where info.supply_demand_id = #{id}  limit  1)
                as t;
    </select>
    <select id="fetchHouseNameByDemandId" resultType="java.lang.String">
        select house_name from demand_info_rel info,base_house house
        where  info.house_id = house.id and  info.supply_demand_id = #{id}  limit  1
    </select>
    <select id="getBaseHouseByDemandId" resultType="com.sandu.api.basesupplydemand.model.BaseHouse">
        select house.* from demand_info_rel info,base_house house
        where  info.house_id = house.id and  info.supply_demand_id = #{id}  limit  1
    </select>
    <select id="listSpaceDetailsByHouseId" resultType="com.sandu.api.basesupplydemand.model.SpaceCommon">
       SELECT
        case sd.att1
            when 1 then '厅'
            when 2 then '室'
        end as space_name,
               sc.origin,
               count(1) as spaceNum
        FROM base_house bh
               inner JOIN house_space_new hsn ON bh.id = hsn.house_id
               inner join space_common sc on hsn.standard_space_id = sc.id and sc.is_deleted = 0
               inner join sys_dictionary sd on sc.space_function_id = sd.value and sd.type = 'houseType'
        WHERE bh.id = #{houseId}
        group by sc.origin, sd.att1;
    </select>
    <select id="fetchPlanNameByIdAndType" resultType="java.lang.String">
        select
        case
            when plan_type  in  (2,4) then (select plan_name from full_house_design_plan where id = t.plan_id)
            when plan_type =  1 then (select plan_name from design_plan_recommended where id = t.plan_id)
            when plan_type =  3 then (select plan_name from design_plan_render_scene where id = t.plan_id)
        end planName
        from
        (select #{id} as plan_id ,#{type} as plan_type)
                as t
    </select>
    <select id="getDemandRelInfo" resultType="com.sandu.api.basesupplydemand.model.DemandInfoRel">
        select * from demand_info_rel where supply_demand_id = #{basesupplydemandId} and (is_deleted is null or is_deleted = 0);
    </select>
    <select id="getBaseHouseByHouseId" resultType="com.sandu.api.basesupplydemand.model.BaseHouse">
        select house.* from base_house house
        where  house.id = #{houseId} and is_deleted = 0  limit  1
    </select>
    <select id="listNodeInfoByContentIdsAndNodeType"
            resultType="com.sandu.api.basesupplydemand.model.NodeDetails">
        select node.id as nodeId,node.content_id ,node.node_type from
        node_info
        node
        where node.node_type = #{nodeType}
        and content_id in
        <foreach collection="contentIds" separator="," open="(" close=")" item="it">
            #{it}
        </foreach>
    </select>
    <select id="getResPicByIds" resultType="com.sandu.api.goods.model.ResPic">
        select id,pic_path
        from supply_demand_pic rp where rp.id in
        <foreach collection="picIds" separator="," item="it" open="(" close=")">
            #{it}
        </foreach>
    </select>

