    <update id="updateNodeDetailsValue">
        update node_info node, node_info_detail detail
        set detail.value = #{value}
        where node.id = detail.node_id
        and node.node_type = #{nodeType} and content_id = #{contentId}
        and detail_type = #{detailsType};
    </update>
    <delete id="deleteByIds">
        update base_supply_demand set is_deleted = 1
        where id IN
        <foreach separator="," open="(" close=")" collection="ids" item="id">
            #{id}
        </foreach>
    </delete>
    <delete id="deleteDemandInfoRel">

        update demand_info_rel set is_deleted =1
        where supply_demand_id = #{id}
        and (
        house_id not in
        <foreach collection="houseIds" item="it" close=")" open="(" separator=",">
            #{it}
        </foreach>
        or
        plan_id not in
        <foreach collection="planIds" item="it" close=")" open="(" separator=",">
            #{it}
        </foreach>
        )
    </delete>

